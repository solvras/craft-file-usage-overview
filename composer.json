{
    "name": "solvr-as/craft-file-usage-overview",
    "type": "craft-plugin",
    "version": "1.0.1",
    "license": "mit",
    "support": {
        "email": "utviklere@solvr.no"
    },
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^3.9.0"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "solvras\\craftcraftfileusageoverview\\": "src/"
        }
    },
    "extra": {
        "handle": "file-usage-overview",
        "name": "File Usage Overview",
        "developer": "Solvr AS",
        "documentationUrl": "",
        "class": "solvras\\craftcraftfileusageoverview\\FileUsageOverview"
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

